{
  "name": "linkedin-archive",
  "version": "1.0.0",
  "description": "Convert LinkedIn posts and comments to HTML and RSS",
  "type": "module",
  "scripts": {
    "lint-html": "npx -y js-beautify@1 '**/*.html' --type html --replace --indent-size 2 --max-preserve-newlines 1 --end-with-newline",
    "lint-prettier": "npx -y prettier@3.5 --print-width 120 --write '**/*.js' '!**/*.min.js' '**/*.md' '**/*.yml'",
    "lint": "npm run lint-html && npm run lint-prettier",
    "build": "node publish.js"
  },
  "dependencies": {
    "csv-parse": "^5.5.6"
  },
  "author": "",
  "license": "MIT"
}
